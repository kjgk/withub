<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
        <pattern>/api/*</pattern>
        <pattern>/index</pattern>
        <pattern>/app/template/*</pattern>
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
